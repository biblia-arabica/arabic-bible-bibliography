        <dc:identifier>
            <dcterms:URI>
                <rdf:value>https://www-1tandfonline-1com-100134eze0486.emedia1.bsb-muenchen.de/doi/abs/10.1080/09596410801923691</rdf:value>
            </dcterms:URI>
